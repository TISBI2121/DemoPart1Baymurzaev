<Window x:Class="KR2_Baymurzaev.Windows.UserAddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KR2_Baymurzaev.Windows"
        mc:Ignorable="d"
        Title="Добавление сотрудника" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="ФИО"/>
            <TextBox x:Name="TBFIO"/>
            <TextBlock Text="Паспорт"/>
            <TextBox x:Name="TBPass"/>
            <TextBlock Text="Адрес"/>
            <TextBox x:Name="TBAddress"/>
            <TextBlock Text="Телефон"/>
            <TextBox x:Name="TBTelephone"/>
            <TextBlock Text="Стек технологий"/>
            <TextBox x:Name="TBStackDesc"/>
            <TextBlock Text="Обязанности"/>
            <TextBox x:Name="TBDutiDesc"/>
            <TextBlock Text="Ставка"/>
            <TextBox x:Name="TBRate"/>
            <TextBlock Text="Логин"/>
            <TextBox x:Name="TBLogin"/>
            <TextBlock Text="Пароль"/>
            <TextBox x:Name="TBPassword"/>
            <Button x:Name="BAdd" Content="Добавить" Click="BAdd_Click"/>
        </StackPanel>
    </Grid>
</Window>
